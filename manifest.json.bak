{

    "manifest_version": 2,
    "name": "reddelstiltskin",
    "version": "0.1",
  
    "description": "switches 'www.reddit.com' to 'old.reddit.com'",
  
    "icons": {
      "48": "icons/icon2.png",
      "96": "icons/icon2-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://www.reddit.com/*"],
        "js": ["reddelstiltskin.js"]
      }
    ]
  
  }